<?xml version="1.0" encoding="UTF-8"?>
<style2tagmap xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="urn:public:dita4publishers.org:namespaces:word2dita:style2tagmap"
    xsi:schemaLocation="urn:public:dita4publishers.org:namespaces:word2dita:style2tagmap ../../org.dita4publishers.doctypes/doctypes/style2tagmap/xsd/style2tagmap.xsd">
    <title>Word Default Styles Style-to-Tag Map</title>
    <documentation> </documentation>
    <styles>
        <output mapType="map"
            prologType="topicmeta"
            name="map"
            doctype-system="map.dtd"
            doctype-public="-//OASIS//DTD DITA BookMap//EN"/>
        
        <output topicType="concept"
            bodyType="conbody"
            name="concept"
            doctype-system="concept.dtd"
            doctype-public="-//OASIS//DTD DITA Concept//EN"/>
        
        <output name="task"
            doctype-system="task.dtd"
            doctype-public="-//OASIS//DTD DITA Task//EN"/>
        
        <output name="reference"
            doctype-system="reference.dtd"
            doctype-public="-//OASIS//DTD DITA Reference//EN"/>
        
        
        <paragraphStyle
            styleName="Title"
            level="0"
            structureType="mapTitle"
            tagName="title">
                <mapProperties
                format="map"
                prologType="topicmeta"
                tagName="title"/>
        </paragraphStyle>
        
        <paragraphStyle
            styleName="mapattribute_id"
            structureType="skip"
            containingTopic="root"/>  
        
        <paragraphStyle
            styleName="mapattribute_rev"
            structureType="skip"
            containingTopic="root"/>   
        
        <paragraphStyle
            styleName="mapdata_DocType"
            topicZone="prolog"
            structureType="prologData"
            baseClass="- topic/data "
            containingTopic="root"
            level="0"
            tagName="data" 
            dataName="DocType"
            putValueIn="value"/>
        
        <paragraphStyle
            styleName="ShortDesc"
            secondStructureType="topicTitle"
            tagName="shortdesc"
            topicZone="shortdesc"
            level="1">
                <topicrefProperties
                topicrefType="topicref"/>
                <topicProperties
                topicDoc="yes"
                topicType="reference"
                format="reference"
                outputclass="Dx:ShortDescription"/>
        </paragraphStyle>
        
        <paragraphStyle
            styleName="CovHead1"
            structureType="topicTitle"
            tagName="title"
            topicZone="body"
            level="1">
                <topicrefProperties
                topicrefType="topicref"/>
                <topicProperties
                topicDoc="yes"
                topicType="concept"
                bodyType="conbody"
                format="concept"
                outputclass="Dx:CoverPage"/>
        </paragraphStyle>
        
        <paragraphStyle
            styleName="CovHead2"
            structureType="topicTitle"
            tagName="title"
            topicZone="body"
            level="2">
                <topicrefProperties
                topicrefType="topicref"/>
                <topicProperties
                topicDoc="yes"
                topicType="concept"
                bodyType="conbody"
                format="concept"
                outputclass="Dx:CoverPage"/>
        </paragraphStyle>
        <paragraphStyle
            styleName="Heading 1"
            structureType="topicTitle"
            tagName="title"
            topicZone="body"
            level="1">
                <topicrefProperties
                topicrefType="topicref"/>
                <topicProperties
                topicDoc="yes"
                topicType="concept"
                bodyType="conbody"
                format="concept"/>
        </paragraphStyle>
        
        <paragraphStyle
            styleName="Heading 2"
            structureType="topicTitle"
            tagName="title"
            topicZone="body"
            level="2">
                <topicrefProperties
                topicrefType="topicref"/>
                <topicProperties
                topicDoc="yes"
                topicType="concept"
                bodyType="conbody"
                format="concept"/>
        </paragraphStyle>
        
        <paragraphStyle
            styleName="Heading 3"
            structureType="topicTitle"
            tagName="title"
            topicZone="body"
            level="3">
                <topicrefProperties
                topicrefType="topicref"/>
                <topicProperties
                topicDoc="yes"
                topicType="concept"
                bodyType="conbody"
                format="concept"/>
        </paragraphStyle>
        
        <paragraphStyle
            styleName="Heading 4"
            structureType="topicTitle"
            tagName="title"
            topicZone="body"
            level="4">
                <topicrefProperties
                topicrefType="topicref"/>
                <topicProperties
                topicDoc="yes"
                topicType="concept"
                bodyType="conbody"
                format="concept"/>
        </paragraphStyle>
        
        <paragraphStyle styleName="Normal" tagName="p" topicZone="body" level="1"/>
        <paragraphStyle styleName="Caption" tagName="title" topicZone="body" level="1"/>
        <paragraphStyle styleName="Codeblock" tagName="codeblock" topicZone="body" level="1"/>
        <paragraphStyle styleName="Note" tagName="note" topicZone="body" level="1"/>
        
        <paragraphStyle styleName="TOC 1" structureType="skip"/>
        <paragraphStyle styleName="TOC 2" structureType="skip"/>
        <paragraphStyle styleName="TOC 3" structureType="skip"/>
        <paragraphStyle styleName="TOC 4" structureType="skip"/>
        <paragraphStyle styleName="TOC 5" structureType="skip"/>
        <paragraphStyle styleName="TOC 6" structureType="skip"/>
        <paragraphStyle styleName="TOC 7" structureType="skip"/>
        <paragraphStyle styleName="TOC 8" structureType="skip"/>
        <paragraphStyle styleName="TOC 9" structureType="skip"/>
        
        <paragraphStyle styleName="MTConvertedEquation"
            containerType="equation-block"
            tagName="mathml"
            level="1"
            structureType="block"
            topicZone="body" />
        
        <paragraphStyle styleName="List Bullet"
            containerType="ul"
            tagName="li"
            level="1"
            structureType="block"
            topicZone="body"/>
        
        <paragraphStyle styleName="List Bullet 2"
            containerType="ul"
            tagName="li"
            level="2"
            structureType="block"
            topicZone="body"/>
        
        <paragraphStyle styleName="List Number"
            containerType="ol"
            tagName="li"
            level="1"
            structureType="block"
            topicZone="body"/>
        
        <paragraphStyle styleName="List Number 2"
            containerType="ol"
            tagName="li"
            level="2"
            structureType="block"
            topicZone="body"/>
        
        <characterStyle styleName="Code" tagName="codeph" structureType="block"/>
        <characterStyle styleName="Hyperlink" structureType="xref"/>
        <characterStyle styleName="Strong" tagName="b" structureType="block"/>
        <characterStyle styleName="Subscript" tagName="sub" structureType="block"/>
        <characterStyle styleName="Superscript" tagName="sup" structureType="block"/>
		<characterStyle styleName="Overline" tagName="overline" structureType="block"/>
        
    </styles>
</style2tagmap>
